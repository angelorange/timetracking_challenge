<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Owner</th>
      <th scope="col">Location</th>
      <th scope="col">Created_at</th>
    </tr>
  </thead>
  <tbody>
    <% @time_trackings.each do |t| %>
      <tr>
        <th><%= link_to t.id, time_tracking_path(t) %></th>
        <td><%= t.owner %></td>
        <td><%= t.geofencing.location %></td>
        <td><%= t.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>